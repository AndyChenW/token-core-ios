{
  "name": "TokenCore",
  "version": "0.1",
  "summary": "Blockchain Library for imToken",
  "description": "Token Core Library powering imToken iOS app.",
  "homepage": "https://token.im",
  "license": {
    "type": "Apache License, Version 2.0",
    "file": "LICENSE"
  },
  "authors": {
    "James Chen": "james@ashchan.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "git": "https://github.com/consenlabs/token-core-ios.git",
    "tag": "0.1"
  },
  "source_files": [
    "TokenCore/Classes/**/*",
    "TokenCore/Vendor/**/*"
  ],
  "swift_version": "4.0",
  "preserve_paths": "TokenCore/Modules",
  "pod_target_xcconfig": {
    "SWIFT_INCLUDE_PATHS": "${PODS_ROOT}/../../TokenCore/Modules",
    "OTHER_LDFLAGS": "-lObjC",
    "SWIFT_OPTIMIZATION_LEVEL": "-Owholemodule"
  },
  "dependencies": {
    "CryptoSwift": [
      "0.9.0"
    ],
    "BigInt": [
      "3.0.0"
    ],
    "GRKOpenSSLFramework": [

    ],
    "secp256k1.swift": [

    ]
  }
}
